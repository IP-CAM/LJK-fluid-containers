<?xml version="1.0" encoding="UTF-8"?>
<modification>
    <id>Hide Product Tags</id>
    <version>0.1</version>
    <vqmver>2.X</vqmver>
    <author>Lucas Krupinski - lucasjkr@me.com</author>
    <comment></comment>
    
    <file name="catalog/view/theme/*/template/*/*.tpl">
        <operation info="Hide label" error="skip">
            <search position="replace"><![CDATA[class="container ]]></search>
            <add><![CDATA[class="container-fluid ]]></add>
        </operation>
        <operation info="Hide label" error="skip">
            <search position="replace"><![CDATA[class="container"]]></search>
            <add><![CDATA[class="container-fluid"]]></add>
        </operation>
    </file>

</modification>
